<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<!--This layer.xml work in Jeddict combined release "org.netbeans.jpa.modeler" , otherwsie move to "org.netbeans.jpa.modeler.installer"--> 
<filesystem>
    <folder name="Services">
        <folder name="AutoupdateType">
            <file name="org.netbeans.jpa.modeler.instance">
                <attr name="displayName" bundlevalue="org.netbeans.jpa.modeler.update.version.Bundle#Services/AutoupdateType/org.netbeans.jpa.modeler.instance"/>
                <attr name="enabled" boolvalue="true"/>
                <attr name="instanceCreate" methodvalue="org.netbeans.modules.autoupdate.updateprovider.AutoupdateCatalogFactory.createUpdateProvider"/>
                <attr name="instanceOf" stringvalue="org.netbeans.spi.autoupdate.UpdateProvider"/>
                <attr name="url" bundlevalue="org.netbeans.jpa.modeler.update.version.Bundle#org.netbeans.jpa.modeler"/>
            </file>
        </folder>
    </folder>
</filesystem>